@article{moronMacroNanoScale2016,
  title = {Macro and Nano Scale Modelling of Water--Water Interactions at Ambient and Low Temperature: Relaxation and Residence Times},
  shorttitle = {Macro and Nano Scale Modelling of Water--Water Interactions at Ambient and Low Temperature},
