@article{vandevondeleQuickstepFastAccurate2005,
  title = {Quickstep: {{Fast}} and Accurate Density Functional Calculations Using a Mixed {{Gaussian}} and Plane Waves Approach},
  shorttitle = {Quickstep},
  author = {VandeVondele, Joost and Krack, Matthias and Mohamed, Fawzi and Parrinello, Michele and Chassaing, Thomas and Hutter, J{\"u}rg},
  year = {2005},
