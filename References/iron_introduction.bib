   journal = {Journal of Computational Chemistry},
   keywords = {Boltzmann,MarvinSpace,Poisson,electrostatic potential,explicit solvent,implicit solvent,membrane,molecular dynamics,visualization},
   month = {8},
   pages = {1859-1865},
   pmid = {18351591},
   publisher = {John Wiley & Sons, Ltd},
   title = {{CHARMM-GUI}: A web-based graphical user interface for {CHARMM}},
   volume = {29},
   year = {2008},
}
