   month = {8},
   pages = {11482-11486},
   pmid = {12177411},
   publisher = {Proc Natl Acad Sci U S A},
   title = {Loss of stearoyl-{CoA} desaturase-1 function protects mice against adiposity},
   volume = {99},
   year = {2002},
}
@article{Snaebjornsson2020,
   abstract = {<p>Altered lipid metabolism is among the most prominent metabolic alterations in cancer. Enhanced synthesis or uptake of lipids contributes to rapid cancer cell growth and tumor formation. Lipids are a highly complex group of biomolecules that not only constitute the structural basis of biological membranes but also function as signaling molecules and an energy source. Here, we summarize recent evidence implicating altered lipid metabolism in different aspects of the cancer phenotype and discuss potential strategies by which targeting lipid metabolism could provide a therapeutic window for cancer treatment.</p>},
   author = {Marteinn Thor Snaebjornsson and Sudha Janaki-Raman and Almut Schulze},
   doi = {10.1016/J.CMET.2019.11.010},
